<fileDesc>
    <titleStmt>
        <title level="a" type="main"><%= entity.name %></title>
        <% create_deed = entity.deeds.where(:deed_type => Deed::ENTITY_CREATE).first %>
        <% if create_deed %>
          <respStmt>
            <resp n="created">Created by:</resp>
            <name><%= create_deed.user.name %></name>
          </respStmt>        
        <% end %>
        <% editors = entity.deeds.where.not(:deed_type => Deed::ENTITY_CREATE).map { |d| d.user.name }.uniq %>
        <% editors.each do |editor| %>
          <respStmt>
            <resp n="edited">Edited by:</resp>
            <name><%= editor %></name>
          </respStmt>
        <% end %>
    </titleStmt>
    <publicationStmt>
        <publisher>Kentucky Historical Society</publisher>
        <pubPlace>Frankfort, Kentucky</pubPlace>
        <availability><p>All research and writing of Civil War Governors of Kentucky Digital Documentary Edition annotations was undertaken by project staff, volunteers, and interns. Each record contains citations to relevant primary and secondary source material. All annotations are copyright Kentucky Historical Society, and should be referenced as such in any published work.</p></availability>
    </publicationStmt>
    <seriesStmt>
        <title level="s" type="main">Civil War Governors of Kentucky Digital Documentary Edition</title>
        <respStmt>
            <resp>Project Director</resp>
            <name>Patrick A. Lewis</name>
        </respStmt>
    </seriesStmt>
    <sourceDesc><ab>Born Digital</ab></sourceDesc>
</fileDesc>
