<div class="container">
<p id="notice"><%= notice %></p>
<h1>Dashboard for  <%= @user.hypothesis_user %> </h1>
<h3>My annotated document</h3>

<table>
  <tbody>
    <% @documents.each do |document| %>
      <tr>
        <td><%= link_to document.cwgk_id, document %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h3>My entities</h3>
<table>
  <tbody>
    <% @user.entities.each do |entity| %>
      <tr>
        <td><%= link_to entity.name, entity %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Entity', new_entity_path %>

<h3>All annotations</h3>
<table>
  <tbody>
    <% @annotations.each do |annotation| %>
      <tr>
        <td><%= link_to annotation.verbatim, annotation %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>